{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FastAPI: Uvicorn",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app.main:app",  // path to your FastAPI app object
          "--reload"
        ],
        "jinja": true,
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "console": "integratedTerminal"
      }
    ]
  }